<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>PortableinnovationEn</servlet-name>
		<servlet-class>ca.portableinnovation.www.PortableinnovationServlet</servlet-class>
		<init-param>
  			<param-name>lang</param-name>
			<param-value>en</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>PortableinnovationFr</servlet-name>
		<servlet-class>ca.portableinnovation.www.PortableinnovationServlet</servlet-class>
		<init-param>
  			<param-name>lang</param-name>
			<param-value>fr</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>PortableinnovationEn</servlet-name>
		<url-pattern>/en</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PortableinnovationFr</servlet-name>
		<url-pattern>/fr</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
